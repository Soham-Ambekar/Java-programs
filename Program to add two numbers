import java.util.*;

public class Main {
    public static void main(String [] args){
        Scanner sc = new Scanner(System.in);
        System.out.println("ENTER FIRST NUMBER :");
        int a = sc.nextInt();

        System.out.println("ENTER SECOND NUMBER : ");
        int b = sc.nextInt();

        System.out.println("ADDITION IS : ");
        int sum = a+b;


        System.out.println(sum);

    }
}
