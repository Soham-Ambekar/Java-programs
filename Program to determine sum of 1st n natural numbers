
import java.util.*;

public class Main {
    public static void main(String [] args){
        //sum of first n natural numbers
        Scanner sc = new Scanner(System.in);

        System.out.println("Enter your nth number : ");
        int n = sc.nextInt();

        int sum = 0;
        for(int i=0;i<=n;i++){
            sum=sum+i;
            System.out.println(sum);
        }

    }
}
