import java.util.*;

public class Main {
    public static void main(String [] args){
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter your Number :");
        int a = sc.nextInt();
        if(a%2 ==0){
            System.out.println("Its even");}
        else{
            System.out.println("its odd");
        }

    }
}
